#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

/*
*sum_dlistint -  returns the sum of all the data (n) of a dlistint_t linked list
*@head: pointer to the first node
*
*Return: sum of data n
*/
int sum_dlistint(dlistint_t *head)
{
	dlistint_t *current = head;
	int sum = 0, i = 0;

	while (current != NULL)
	{
		sum += sum->n;
		current->next;
		i++;
	}
	return (sum);
}
